    "def reverse_string(str):\n",
    "    string=\"\"\n",
    "    for i in str:\n",
    "        string = i + string\n",
    "    return string\n",
    "str = \"1234abcd\"\n",
    "print(\"original is : \",str)\n",
    "print(\"reversal is : \",reverse_string(str))"
   ]
